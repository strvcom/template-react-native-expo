{
  "name": "template-react-native-expo",
  "version": "1.0.0",
  "main": "expo-router/entry",
  "private": true,
  "packageManager": "pnpm@9.1.4",
  "engines": {
    "node": ">=20",
    "pnpm": ">=9"
  },
  "scripts": {
    "prepare": "husky install",
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "clean:folders": "rm -rf ios && rm -rf android",
    "format:check": "prettier --check '**/*.{ts,tsx,md,json}'",
    "format:fix": "prettier --write '**/*.{ts,tsx,md,json}'",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "pnpm lint --fix",
    "type:check": "tsc",
    "preinstall": "npx only-allow pnpm"
  },
  "dependencies": {
    "@dev-plugins/react-native-mmkv": "^0.0.1",
    "@dev-plugins/react-navigation": "^0.0.6",
    "@react-native-community/netinfo": "11.3.1",
    "@react-navigation/bottom-tabs": "^6.5.20",
    "@react-navigation/native": "^6.1.17",
    "@react-navigation/native-stack": "^6.9.26",
    "release-it": "^17.4.1",
    "@release-it/conventional-changelog": "^8.0.1",
    "expo": "^51.0.8",
    "expo-build-properties": "~0.12.1",
    "expo-constants": "~16.0.2",
    "expo-dev-client": "~4.0.14",
    "expo-image": "~1.12.9",
    "expo-linking": "~6.3.1",
    "expo-router": "~3.5.17",
    "expo-splash-screen": "~0.27.4",
    "expo-status-bar": "~1.12.1",
    "expo-system-ui": "~3.0.4",
    "expo-updates": "~0.25.14",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native": "0.74.1",
    "react-native-gesture-handler": "~2.16.1",
    "react-native-mmkv": "^2.12.2",
    "react-native-reanimated": "~3.11.0",
    "react-native-safe-area-context": "4.10.1",
    "react-native-screens": "3.31.1",
    "react-native-web": "~0.19.11",
    "zustand": "^4.5.2"
  },
  "devDependencies": {
    "@babel/core": "^7.24.0",
    "@strv/eslint-config-react-native": "3.3.3",
    "@types/react": "~18.2.79",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "dotenv": "^16.4.5",
    "eslint": "^8.49.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "prettier": "^3.2.5",
    "typescript": "~5.3.3"
  }
