{
  "name": "template-react-native-expo",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "yarn expo start --dev-client",
    "android": "yarn expo run:android",
    "ios": "yarn expo run:ios",
    "web": "yarn expo start --web",
    "clean-folders": "rm -rf ios && rm -rf android",
    "format:check": "prettier --check '**/*.{ts,tsx,md,json}'",
    "format:fix": "prettier --write '**/*.{ts,tsx,md,json}'",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "yarn lint --fix",
    "type-check": "tsc",
    "prepare": "husky install"
  },
  "dependencies": {
    "@react-native-community/netinfo": "9.3.7",
    "@react-navigation/bottom-tabs": "^6.4.0",
    "@react-navigation/devtools": "^6.0.10",
    "@react-navigation/native": "^6.0.13",
    "@react-navigation/native-stack": "^6.9.1",
    "expo": "~48.0.4",
    "expo-build-properties": "~0.5.1",
    "expo-constants": "~14.2.1",
    "expo-dev-client": "~2.1.3",
    "expo-linking": "~4.0.1",
    "expo-splash-screen": "~0.18.1",
    "expo-status-bar": "~1.4.4",
    "expo-system-ui": "~2.2.1",
    "expo-updates": "~0.16.3",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native": "0.71.3",
    "react-native-gesture-handler": "~2.9.0",
    "react-native-mmkv": "^2.4.1",
    "react-native-reanimated": "~2.14.4",
    "react-native-safe-area-context": "4.5.0",
    "react-native-screens": "~3.20.0",
    "react-native-web": "~0.18.10",
    "zustand": "^4.1.2"
  },
  "devDependencies": {
    "@babel/core": "^7.19.3",
    "@strv/eslint-config-react-native": "^3.2.0",
    "@strv/eslint-config-typescript": "^3.2.0",
    "@types/react": "~18.0.27",
    "babel-plugin-module-resolver": "^4.1.0",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "dotenv": "^16.0.1",
    "eslint": "^8.25.0",
    "eslint-config-prettier": "^8.5.0",
    "husky": "^8.0.0",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "react-native-dotenv": "^3.3.1",
    "react-native-flipper": "^0.145.0",
    "react-native-flipper-zustand": "^2.0.1",
    "react-native-mmkv-flipper-plugin": "^1.0.0",
    "typescript": "^4.9.4"
  },
  "private": true
}
