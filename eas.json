{
  "cli": {
    "version": ">= 3.13.3",
    "appVersionSource": "remote"
  },
  "build": {
    "base": {
      "ios": {
        "resourceClass": "medium"
      }
    },
    "dev": {
      "extends": "base",
      "developmentClient": true,
      "distribution": "internal",
      "environment": "development",
      "android": {
        "buildType": "apk"
      },
      "channel": "dev",
      "env": {
        "EXPO_PUBLIC_APP_ENV": "dev"
      }
    },
    "dev-sim": {
      "extends": "dev",
      "environment": "development",
      "ios": {
        "simulator": true
      }
    },
    "staging": {
      "extends": "base",
      "distribution": "store",
      "channel": "staging",
      "autoIncrement": true,
      "environment": "preview",
      "env": {
        "EXPO_PUBLIC_APP_ENV": "staging"
      }
    },
    "production": {
      "extends": "base",
      "distribution": "store",
      "channel": "production",
      "environment": "production",
      "env": {
        "EXPO_PUBLIC_APP_ENV": "production"
      }
    }
  },
  "submit": {
    "staging": {
      "ios": {
        "ascAppId": "6670311845",
        "appleTeamId": "EAVQTDVRT7"
      },
      "android": {
        "track": "internal",
        "releaseStatus": "draft"
      }
    }
  }
}
