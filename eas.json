{
  "cli": {
    "version": ">= 3.13.3"
  },
  "build": {
    "base": {
      "ios": {
        "resourceClass": "medium"
      }
    },
    "dev": {
      "extends": "base",
      "developmentClient": true,
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      },
      "channel": "dev",
      "env": {
        "EXPO_PUBLIC_APP_ENV": "dev"
      }
    },
    "dev-sim": {
      "extends": "dev",
      "ios": {
        "simulator": true
      }
    },
    "staging": {
      "extends": "base",
      "distribution": "store",
      "config": "build-and-slack.yml",
      "channel": "staging",
      "env": {
        "EXPO_PUBLIC_APP_ENV": "staging"
      }
    },
    "production": {
      "extends": "base",
      "distribution": "store",
      "channel": "production",
      "env": {
        "EXPO_PUBLIC_APP_ENV": "production"
      }
    }
  },
  "submit": {}
}
